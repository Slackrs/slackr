<!DOCTYPE html>
<html>
<head>
  <title>Slackr</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="header">
		<p>S &nbsp;L &nbsp;A &nbsp;C &nbsp;K &nbsp;R</p>
		<!-- <p>s&nbsp;l&nbsp;a&nbsp;c&nbsp;k&nbsp;r</p> -->
		<!-- <p>SLACKR</p> -->
		<div class="tagline">It's 9 AM; do you know where your students are?</div>
	</div>

	<div class="container">
		<div class="nav">
				<%= form_tag('/login', method: 'delete') do %>
					<%= submit_tag('LOG OUT') %>
					<button type="submit">LOG OUT</button>
				<% end %> 

		</div>

		<div class="content">
			<%= yield %>
		</div>
	</div>

	<div class="footer">

	</div>
	

</body>
</html>
