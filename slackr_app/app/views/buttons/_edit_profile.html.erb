<% if @current_user.type == "Producer" %>
<%= button_to "edit profile" ,  edit_producer_path(@current_user),  :class => "edit_profile" %>
<% elsif @current_user.type == "Instructor" %>
<%= button_to "edit profile" , edit_cohort_instructor_path(@current_user), :class => "edit_profile" %>
<% else %>
<%= button_to "edit profile" , edit_cohort_student_path(@current_user), :class => "edit_profile" %>
<% end %>