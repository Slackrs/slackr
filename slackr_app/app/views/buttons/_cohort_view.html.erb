<% if @current_user.type == "Instructor" %>
	<%= button_to "Cohort View", cohort_instructor_path(@current_user.cohort_id,@current_user.id) %>
<% elsif @current_user.type == "Producer" %>
	<%= button_to "Cohort View", cohort_path(params[:id]) %>
<% else %>
<!-- this will be essentially a "back" button to redirect to student#show -->
	<%= button_to "Back", cohort_student_path(@current_user.cohort_id ,@current_user.id) %>
<% end %>