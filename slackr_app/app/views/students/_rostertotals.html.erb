<div class="container-fluid">
<nav class="nav navbar-default navbar-static-top ">
<h1 class="text-center">Slackr</h1>
</nav>
	<div class="row">
		<div class="col-md-4">
			<aside>
				
			</aside>
		</div>
		<div class="col-md-8 table-area">
			<table class="table table-condensed table-striped table-hover">
				<thead>
					<tr>
					<th>Name</th>
					<th>Presents</th>
					<th>Lates</th>
					<th>Absences</th>
					<th>Excuseds</th>
				</tr>
			</thead>
			<tbody >
					<% @students.each do |student| %>
						<% if student.flagged? %>
							<tr class="danger">
						<% else %>
							<tr>
						<% end %>
								<td><%= link_to "#{student.name}", cohort_student_path(student.cohort_id, student.id) %></td>
								<td><%= student.presents %></td>
								<td><%= student.lates %></td>
								<td><%= student.absences %></td>
								<td><%= student.absences %></td>
						</tr>
				<% end %>
			</tbody>
		</table>
		</div>
	</div>
</div>
