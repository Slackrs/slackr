<table class="table-striped">
	<tr>
		<th>Date</th>
		<th>Late</th>
		<th>Absent</th>
		<th>Excused</th>
	</tr>
	<% @attendances.each do |attendance| %>
	<tr>
		<td><%= attendance.date %></td>
		<td><%= attendance.late %></td>
		<td><%= attendance.absent %></td>
		<td><%= attendance.excused %></td>
	</tr>
	<% end %>
</table>